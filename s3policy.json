{
    "Version": "2012-10-17",
    "Id": "PolicyForCloudFrontPrivateContent",
    "Statement": [
        {
            "Sid": " Grant a CloudFront Origin Identity access to support private content",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::cloudfront:user/CloudFront Origin Access Identity E1RC147RJLU46U"
            },
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::47ij94-f9nkbjm/*"
        }
    ]
}
